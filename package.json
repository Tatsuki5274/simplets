    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint  'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "amplify:scriptgraphql": "cp src/API.ts amplify/backend/function/scriptGraphQL/src && cp -R src/graphql amplify/backend/function/scriptGraphQL/src/ && cd amplify/backend/function/scriptGraphQL/src",
    "amplify:resolvers": "cp src/API.ts amplify/backend/function/resolvers/src && cp -R src/graphql amplify/backend/function/resolvers/src/ && cd amplify/backend/function/resolvers",
    "amplify:dbstream": "cp src/API.ts amplify/backend/function/dbStream/src && cp -R src/graphql amplify/backend/function/dbStream/src/ && cd amplify/backend/function/dbStream",
    "invoke:resolvers": "npm run amplify:resolvers && amplify mock function resolvers --event src/event.json --timeout 60",
    "invoke:dbstream": "npm run amplify:dbstream && amplify mock function dbStream --event src/event.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
